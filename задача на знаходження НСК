#include <stdio.h>

int gcd(int a, int b) {
    if (b == 0)
        return a;
    else
        return gcd(b, a % b);
}

int lcm(int a, int b) {
    return a * b / gcd(a, b);
}

int main() {
    int n;
    printf("Введіть кількість натуральних чисел:\n");
    scanf("%d", &n);
    
    int arr[n];
    printf("Введіть %d натуральних чисел, розділені пробілами:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int result = arr[0];
    for (int i = 1; i < n; i++) {
        result = lcm(result, arr[i]);
    }

    printf("Найменше спільне кратне заданих чисел: %d\n", result);

    return 0;
}
